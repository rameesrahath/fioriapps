<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout">
	<Dialog title="{i18n>select}">
		<content>
			<VBox justifyContent="Center">
				<RadioButtonGroup columns="1" selectedIndex="1" class="sapUiMediumMarginBottom">
					<buttons>
						<RadioButton text="{i18n>tile}"/>
						<RadioButton text="{i18n>targetmapping}"/>
					</buttons>
				</RadioButtonGroup>
			</VBox>
		</content>
		<beginButton>
			<Button text="{i18n>ok}" press="handletiletargetokpress"/>
		</beginButton>
		<endButton>
			<Button text="{i18n>cancel}" press="handletiletargetCancelpress"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>
